{
    "routes": [
        {
            "component": "homeComponent",
            "path": "home",
            "children": [
                {
                    "component": "displaypaeComponent",
                    "path": "display"
                },
                {
                    "path": "category",
                    "component": "cateoryComponent"
                },
                {
                    "component": "aboutUsComponent",
                    "path": "about"
                },
                {
                    "component": "artworkComponent",
                    "path": "artwork"
                },
                {
                    "path": "shop",
                    "component": "BuyArtComponent"
                },
                {
                    "path": "post/sales",
                    "component": "postComponent"
                },
                {
                    "component": "eventsComponent",
                    "path": "events"
                },
                {
                    "component": "postButtonsComponent",
                    "path": "post"
                },
                {
                    "component": "postEventsComponent",
                    "path": "post/postevents"
                },
                {
                    "component": "artistsComponent",
                    "path": "art"
                }
            ]
        },
        {
            "path": "login",
            "component": "loginComponent"
        },
        {
            "component": "registerComponent",
            "path": "register"
        },
        {
            "path": "''",
            "redirectTo": "/home",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
